             var CandID = $(this).attr('id');
             $.ajax({
            url: "@Url.Action("EditCandidate", "Placement")",
                 data: { "CandID": CandID},
                 success: function (data) {

                $('#mdlEditCandidate').html(data);
                $(".popupTrigger").click();
            }

        });
      });

    });

    $('.askfeedback').click(function () {
    if (confirm("Are you sure?"))
        {

        var idValue = $(this).attr('id');

                $.ajax({
                    url: "@Url.Action("AskForFeedback", "Placement")",
                    contentType: "application/json",
                    data: JSON.stringify({
                        page: 1,
                        CandID: idValue

                    }),
                    dataType: "text",
                    type: "POST",
                    async: true,
                    success: function (data) {
                        toastr.success("Candidate added for feedback in tracker.");
                        location.reload();
                    },
                    error: function (data) {

                    },
                    complete: function () {

                    }
                });
    }
            });

</script>
